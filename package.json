{
  "name": "polls",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "generate": "prisma generate",
    "postinstall": "npm run generate",
    "start": "next start",
    "lint": "npm run lint:check",
    "lint:check": "npm run eslint:check && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "eslint:check": "next lint",
    "eslint:fix": "next lint --fix",
    "prettier:fix": "prettier --write \"{app,lib,components}/**/*.{ts,mjs,cjs,tsx,mdx,json}\" --cache",
    "prettier:check": "prettier --check \"{app,lib,components}**/*.{ts,mjs,cjs,tsx,mdx,json}\" --cache",
    "pages:build": "pnpm next-on-pages",
    "preview": "pnpm pages:build && wrangler pages dev",
    "deploy": "pnpm pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@cloudflare/next-on-pages": "1.11.0",
    "@cloudflare/workers-types": "^4.20240419.0",
    "@headlessui/react": "1.7.19",
    "@heroicons/react": "2.1.3",
    "@prisma/adapter-d1": "5.12.1",
    "@prisma/client": "5.13.0",
    "@radix-ui/react-icons": "1.3.0",
    "@radix-ui/react-slot": "1.0.2",
    "@types/node": "20.12.7",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "autoprefixer": "10.4.19",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "date-fns": "3.6.0",
    "lucide-react": "0.372.0",
    "next": "14.2.2",
    "prisma": "5.13.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "2.3.0",
    "tailwindcss": "3.4.3",
    "tailwindcss-animate": "1.0.7",
    "zod": "3.23.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.2.2",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "14.2.2",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-next-on-pages": "^1.11.1",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.4.5",
    "vercel": "^34.1.1",
    "wrangler": "^3.51.2"
  }
}
